
public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		Node node = new Node(1);
		
		for(int i = 2; i<=5; i++){
			addNode(node,i);
		}
		
		printList(node);
		System.out.println("------");
		printList(swapNodes(node));
	}
	
	
	public static Node swapNodes(Node head){
		if(head == null){
            return null;
        }
        
        Node aux = head;
        Node aux2;
        
        
        while(aux.next != null){
        	aux2 = aux.next;
            aux.next = aux2.next;
            aux2.next = aux;
            aux = aux.next;
        }
    
        return head;    
	}
	
	
	public static Node addNode(Node node, int data){
		if(node == null){
			return null;
		}
		
		Node aux = node;
		
		while(aux.next != null){
			aux = aux.next;
		}
		
		aux.next = new Node(data);
		
		return node;
		
	}
	
	
	public static void printList(Node node){
		if(node == null){
			return;
		}
		
		while(node != null){
			System.out.print(node.data + " ");
			node = node.next;
		}
	
	}
}
